generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Photos {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  alt        String
  desktopUrl String
  tabletUrl  String
  mobileUrl  String
}

model Renders {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  name       String
  alt        String
  desktopUrl String
  mobileUrl  String
  width      Int
  height     Int
}
