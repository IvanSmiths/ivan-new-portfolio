{
  "name": "ivan-new-portfolio",
  "version": "3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tests:dev": "TURSO_CONNECTION_URL=file:portfolio.db TURSO_TOKEN=none next dev",
    "snapshots:run": "npx playwright test",
    "snapshots:ui": "npx playwright test --ui",
    "snapshots:update": "npx playwright test --update-snapshots",
    "analyze": "cross-env ANALYZE=true next build",
    "drizzle-studio": "npx drizzle-kit studio --port 9000",
    "migration:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "migration:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "migration:push": "drizzle-kit push --config=drizzle.config.ts",
    "migration:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "cypress:open": "cypress open --config-file=cypress.config.ts",
    "cypress:run": "cypress run --config-file=cypress.config.ts"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0-alpha.34",
    "@gsap/react": "^2.1.1",
    "@libsql/client": "^0.6.2",
    "@next/mdx": "^14.2.13",
    "@types/mdx": "^2.0.13",
    "bright": "^0.8.5",
    "drizzle-orm": "^0.30.10",
    "gsap": "^3.12.5",
    "next": "^14.2.13",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.13",
    "@playwright/test": "^1.47.2",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.10",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.13",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
